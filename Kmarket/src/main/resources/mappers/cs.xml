<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace = mapper파일의 아이디 값 -->
<mapper namespace="kr.co.kmarket.dao.CsDAO">
	<select id="selectNotice" resultType="kr.co.kmarket.vo.CsVO">
		SELECT * FROM `km_cs_notice`;
	</select>
	<select id="selectNoticeByCate" resultType="kr.co.kmarket.vo.CsVO">
		SELECT * FROM `km_cs_notice` WHERE `group`= #{group};
	</select>
	<select id="selectQna" resultType="kr.co.kmarket.vo.CsVO">
		SELECT * FROM `km_cs_qna`;
	</select>
</mapper>